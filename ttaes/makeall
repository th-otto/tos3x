#!sh
ALCYON='c:\devtools\alcyon'

BIN="$ALCYON\bin\"
LIB="$ALCYON\lib\"
INC="$ALCYON\include"

COUNTRY=usa

CDEF_usa=USA
CDEF_uk=UK
CDEF_grm=GERMANY
CDEF_fre=FRENCH
CDEF_spa=SPAIN
CDEF_ita=ITALY
CDEF_swe=SWEDEN

eval CDEF=\$CDEF_${COUNTRY}


echo CC gemaplib.c
${BIN}cp68 -D${CDEF}=1 gemaplib.c gemaplib.i
${BIN}c068 gemaplib.i gemaplib.1 gemaplib.2 gemaplib.3 -f
${BIN}c168 gemaplib.1 gemaplib.2 gemaplib.s
${BIN}optimize gemaplib.s > NUL:
${BIN}as68 -l -u gemaplib.s
rm -f gemaplib.s gemaplib.i gemaplib.1 gemaplib.2 gemaplib.3
mv gemaplib.o gemaplib.oo

