COBJS7 = writeasc.o writebin.o blkio.o xread.o xwrite.o xopen.o getmode.o
COBJS8 = strupr.o strlwr.o strcpy.o strncpy.o strlen.o strcat.o strncat.o
COBJS9 = strcmp.o stricmp.o qsort.o fseek.o rewind.o ftell.o
COBJS10 = printf.o fprintf.o sprintf.o xdoprtfp.o doprint.o
COBJS11 = fread.o fwrite.o
COBJS12 = ldiv.o lmul.o lrem.o
COBJS13 = aldiv.o almul.o alrem.o uldiv.o
COBJS14 = puts.o fputs.o xttyin.o ttyin.o ttyout.o
COBJS15 = abort.o chmod.o dup.o
