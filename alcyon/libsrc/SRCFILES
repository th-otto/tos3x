CSRCS10 = nf_name.c nf_vers.c nf_debug.c nf_shutd.c nf_ops.c cookie.c
CSRCS11 = printf.c fprintf.c sprintf.c vprintf.c vfprintf.c vsprintf.c xdoprtfp.c doprint.c
