AOBJS2 = $(ASRCS2:.S=.o)

AOBJS = $(AOBJS1) $(AOBJS2)
ASRCS = $(ASRCS1) $(ASRCS2)

CSRCS1 = asctime.c ctime.c localtime.c mktime.c gmtime.c consec.c cnvsec.c timezone.c time.c
