COMA_SRCS = coma.S command.c

COMA_OBJS = coma.o command.o 

FIND_SRCS = find.c

FIND_OBJS = $(FIND_SRCS:.c=.o)

TIME_SRCS = time.c dt.c

TIME_OBJS = time.o dt.o gemdos.o

DATE_SRCS = date.c dt.c

DATE_OBJS = date.o dt.o gemdos.o

LIB1OBJS=lmul.o lrem.o ldiv.o uldiv.o ulrem.o gemdos.o bios.o

SRCS = $(COMA_SRCS) $(FIND_SRC) $(DATE_SRCS) $(TIME_SRCS)

EXTRA_DIST1 = Makefile SRCFILES dt.h
EXTRA_DIST2 = GNUmakefile
